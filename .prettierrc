{
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "jsxBracketSameLine": false,
    "overrides": [
        {
            "files": "*.user.js",
            "options": {
                "parser": "babel"
            }
        }
    ]
}
